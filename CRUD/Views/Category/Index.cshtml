@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-striped" style="width:100%"> 
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>DisplayOrder</th>
        <th>CreatedDateTime</th>
    </tr>

    @foreach(Category m in Model)
    {
        <tr>
            <td>@m.Id</td>
            <td>@m.Name</td>
            <td>@m.DisplayOrder</td>
            <td>@m.CreatedDateTime</td>
        </tr>
    }

</table>